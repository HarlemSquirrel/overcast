<%= form_with url: forecasts_path, method: :get, class: 'd-flex', role: :search do |form| %>
  <%= form.text_field :q, class: 'form-control me-2', type: 'search', placeholder: 'Address', 'aria-label': 'Address', value: params[:q] %>
  <%= form.submit "Get weather", class: 'btn btn-outline-success' %>
<% end %>
